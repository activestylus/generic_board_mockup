<div id="slideshow" class="jcarousel-skin-default">
  <div id="featured_recruiters" class="jcarousel">
    <ul>
      <% %w(94 68 116 115 105 120).each do |n| %>
      <li>
        <a class="logoslide" style="background-image:url(images/logos/<%=n%>.png); width:<%=n%>px;" href="#">
          <%= n.titleize %>
        </a>
      </li>
      <% end %>
    </ul>
    <a id="slide-left" href="#">«</a>
    <a id="slide-right" href="#">»</a>
  </div>
</div>
<div class="hr break thin"></div>