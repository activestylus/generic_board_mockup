<% @title = "Candidate-Saved-jobs" %>
<div class="candidate">
  <%= partial "shared/sidebar" %>
  <div class="main col" id="saved-jobs">
    <%= partial "shared/candidate_menu", locals: {active: "Saved jobs"} %>
    <div class='info'>
      <h1>Your Saved jobs</h1>
      <table cellspacing="0" cellpadding="0" border="0" class="clean-list">
        <% ['Web Designer', 'Web Developer', 'Web Analyst', 'Senior Ruby Developer'].each do |job| %>
        <tr>
          <td>
            <h3><a href="#"><%= job %></a></h3>
            Marlow, Buckinghamshire - £40,000 - £55,000
          </td>
          <td class="crud"><a href="#">Edit</a></td>
          <td class="crud"><a href="#">Delete</a></td>
        </tr>
        <% end %>

      </table>

      <div class="hr"></div>
    </div>
  </div>
</div>